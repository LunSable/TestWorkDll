<Window x:Class="TestWorkDll.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestWorkDll"
        mc:Ignorable="d"
        Title="Dll_Reader" Height="450" Width="442">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="109*"/>
            <ColumnDefinition Width="325*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="TxBox" HorizontalAlignment="Left" Height="419" TextWrapping="Wrap" VerticalAlignment="Top" Width="233" Margin="91,0,0,0" UndoLimit="5000" Grid.Column="2" BorderBrush="Black"/>
        <Button x:Name="BtnSend" Content="Send" HorizontalAlignment="Right" Margin="0,0,34,272" VerticalAlignment="Bottom" Width="75" Click="BtnSend_Click" Height="42" Grid.ColumnSpan="2"/>
        <TextBox x:Name="TBox2" HorizontalAlignment="Left" Height="63" TextWrapping="Wrap" Text="Введите директорию" VerticalAlignment="Top" Width="195" Grid.ColumnSpan="3" BorderBrush="Black"/>

    </Grid>
</Window>
